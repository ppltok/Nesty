{
  "manifest_version": 3,
  "name": "Babylist API Tester",
  "version": "1.0.0",
  "description": "Test the Babylist scraper API without CORS issues",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "*://*.babylist.com/*",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Test Babylist API"
  }
}
